{
  "name": "@studiokloek/pixi-texturepacker",
  "version": "0.5.0",
  "description": "Pack sprites for PixiJS using TexturePacker CLI.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "github:studiokloek/pixi-texturepacker",
  "bugs": {
    "url": "https://github.com/studiokloek/pixi-texturepacker/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "$npm_package_bin_pixi_texturepacker ./test/assets.json",
    "watch": "rollup -c -w",
    "release": "np"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "dist/*",
    "bin/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "pixi-texturepacker": "./bin/index.js"
  },
  "devDependencies": {
    "@rollup/plugin-run": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "np": "^5.2.1",
    "rollup": "^1.30.1",
    "rollup-plugin-terser": "^5.2.0",
    "source-map-support": "^0.5.16"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "get-value": "^3.0.1",
    "globby": "^11.0.0",
    "log-symbols": "^3.0.0",
    "meow": "^6.0.0",
    "object.defaults": "^1.1.0",
    "ora": "^4.0.3",
    "pupa": "^2.0.1",
    "sane": "^4.1.0",
    "set-value": "^3.0.1",
    "throttle-debounce": "^2.1.0",
    "uppercamelcase": "^3.0.0"
  }
}
